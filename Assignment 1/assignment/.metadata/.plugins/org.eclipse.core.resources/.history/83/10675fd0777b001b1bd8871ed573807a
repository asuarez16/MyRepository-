package ag1;

public class LargestEvenOrOddSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//For Loop
		
		for(int i=1;i<=10;i++) 
		{
			if(i%2==0) 
			{
			System.out.println(i + " is even");
			}
			else 
			{
			System.out.println(i + " is odd");
			}
		}
		

	}

}
