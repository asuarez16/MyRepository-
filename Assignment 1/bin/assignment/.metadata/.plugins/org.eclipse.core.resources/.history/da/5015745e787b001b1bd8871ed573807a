package ag1;

public class LargestEvenOrOddSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//For Loop
		
		for(int i=1;i<=10;i++) 
		{
			//prints out if numbers between 1 to 10 are even or odd
			if(i%2==0) 
			{
			System.out.println("This is a for loop " + i + " is even");
			}
			else 
			{
			System.out.println("This is a for loop " + i + " is odd");
			}
		}
		
		System.out.println();
		
		//While Loop
		int x=0;
		while(x<=10) 
		{
			
			System.out.println("This is a while loop " + x);
			x++;
		
		}
		
		//Do While Loop
		x=0; 
		do 
		{
			System.out.println("This is a do while loop " + x);
			x++;
		}while(x<=10);
		

		
		
		

	}

}
